---
const canonicalURL = new URL(Astro.url.pathname, Astro.site);
---

<!-- Global Metadata -->
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width,initial-scale=1" />
<meta name="generator" content={Astro.generator} />

<!-- Canonical URL -->
<link rel="canonical" href={canonicalURL} />

<!-- Primary Meta Tags -->
<meta name="title" content="Library UI" />
<meta name="description" content="" />

<link rel="shortcut icon" href="/logo-ui.png" type="image/x-icon" />

<link rel="stylesheet" href="https://crobf-arg.github.io/UI/dist/milig.min.css" />
